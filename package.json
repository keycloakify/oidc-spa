            "default": "./dist/react-spa/index.js"
        },
        "./react-tanstack-start": {
            "types": "./dist/esm/tanstack-start/react/index.d.ts",
            "module": "./dist/esm/tanstack-start/react/index.mjs",
            "import": "./dist/esm/tanstack-start/react/index.mjs",
            "default": "./dist/esm/tanstack-start/react/index.mjs"
        },
        "./angular": {
            "types": "./dist/esm/angular.d.ts",
            "module": "./dist/esm/angular.mjs",
