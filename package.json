{
    "name": "oidc-spa",
    "version": "8.0.5",
    "description": "Openidconnect client for Single Page Applications",
    "repository": {
        "type": "git",
        "url": "git://github.com/keycloakify/oidc-spa.git"
    },
    "scripts": {
        "prepare": "patch-package",
        "build": "tsx scripts/build.ts",
        "tsc:cjs": "tsc --module CommonJS --outDir ./dist",
        "tsc:esm": "tsc --module es2020 --outDir ./dist/esm",
        "watch": "yarn run tsc:esm -w",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "yarn _format --write",
        "format:check": "yarn _format --list-different",
        "link-in-app": "tsx scripts/link-in-app.ts",
        "start-react-router-example": "yarn build && tsx scripts/link-in-examples.ts && cd examples/react-router && rm -rf node_modules/.vite && yarn dev",
        "start-react-router-framework-example": "yarn build && tsx scripts/link-in-examples.ts && cd examples/react-router-framework && rm -rf node_modules/.vite && yarn dev",
        "start-tanstack-router-example": "yarn build && tsx scripts/link-in-examples.ts && cd examples/tanstack-router && rm -rf node_modules/.vite && yarn dev",
        "start-tanstack-router-file-based-example": "yarn build && tsx scripts/link-in-examples.ts && cd examples/tanstack-router-file-based && rm -rf node_modules/.vite && yarn dev",
        "start-multi-providers-example": "yarn build && tsx scripts/link-in-examples.ts && cd examples/multi-providers && rm -rf node_modules/.vite && yarn dev",
        "start-angular-example": "yarn build && tsx scripts/link-in-examples.ts && cd examples/angular && rm -rf .angular/cache && yarn start",
        "size": "bash ./scripts/compute-payload.sh"
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej & u/ddecrulle",
    "license": "MIT",
    "keywords": [
        "OpenID Connect",
        "PKCE",
        "Authorization Code Flow",
        "OIDC",
        "keycloak",
        "keycloak-js",
        "react",
        "angular"
    ],
    "homepage": "https://docs.oidc-spa.dev/",
    "dependencies": {},
    "peerDependencies": {
        "@types/react": "*",
        "react": "*",
        "rxjs": "*",
        "@angular/core": "*",
        "@angular/router": "*"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "@types/react": {
            "optional": true
        },
        "rxjs": {
            "optional": true
        },
        "@angular/core": {
            "optional": true
        },
        "@angular/router": {
            "optional": true
        }
    },
    "devDependencies": {
        "@babel/core": "7.24.9",
        "@babel/preset-env": "7.24.8",
        "@types/jsonwebtoken": "9.0.6",
        "@types/node": "^20.2.1",
        "@types/node-fetch": "2.6.11",
        "@types/node-jose": "1.1.13",
        "@types/react": "19.1.10",
        "babel-loader": "9.1.3",
        "evt": "^2.5.7",
        "husky": "^4.3.8",
        "jsonwebtoken": "9.0.2",
        "lint-staged": "^11.1.1",
        "node-fetch": "2.7.0",
        "node-jose": "2.2.0",
        "oidc-client-ts": "3.1.0",
        "patch-package": "^8.0.0",
        "prettier": "^2.8.8",
        "punycode": "^2.3.1",
        "react": "19.1.1",
        "ts-node": "^10.9.1",
        "tsafe": "^1.8.9",
        "tsx": "^4.15.5",
        "typescript": "5.7.2",
        "webpack": "5.93.0",
        "webpack-cli": "5.1.4",
        "worker-timers": "7.1.7",
        "zod": "^3.23.8",
        "rxjs": "^7.8.2",
        "@angular/core": "^20.3.1",
        "@angular/router": "^20.3.1",
        "@angular/common": "^20.3.1",
        "@angular/platform-browser": "^20.3.1"
    },
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/esm/index.js",
    "exports": {
        ".": {
            "module": "./dist/esm/index.js",
            "default": "./dist/index.js"
        },
        "./mock": {
            "module": "./dist/esm/mock/index.js",
            "default": "./dist/mock/index.js"
        },
        "./react": {
            "module": "./dist/esm/react/index.js",
            "default": "./dist/react/index.js"
        },
        "./angular": {
            "module": "./dist/esm/angular.js",
            "default": "./dist/angular.js"
        },
        "./mock/react": {
            "module": "./dist/esm/mock/react.js",
            "default": "./dist/mock/react.js"
        },
        "./mock/angular": {
            "module": "./dist/esm/mock/angular.js",
            "default": "./dist/mock/angular.js"
        },
        "./entrypoint": {
            "module": "./dist/esm/entrypoint.js",
            "default": "./dist/entrypoint.js"
        },
        "./backend": "./dist/backend.js",
        "./keycloak": {
            "module": "./dist/esm/keycloak/index.js",
            "default": "./dist/keycloak/index.js"
        },
        "./keycloak-js": {
            "module": "./dist/esm/keycloak-js.js",
            "default": "./dist/keycloak-js.js"
        },
        "./tools/decodeJwt": {
            "module": "./dist/esm/tools/decodeJwt.js",
            "default": "./dist/tools/decodeJwt.js"
        },
        "./tools/parseKeycloakIssuerUri": {
            "module": "./dist/esm/tools/parseKeycloakIssuerUri.js",
            "default": "./dist/tools/parseKeycloakIssuerUri.js"
        }
    }
}
