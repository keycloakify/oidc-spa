{
    "name": "oidc-spa",
    "version": "8.7.4",
    "description": "OpenID Connect / OAuth2 solution for Centric Web Applications",
    "repository": {
        "type": "git",
        "url": "git://github.com/keycloakify/oidc-spa.git"
    },
    "scripts": {
        "prepare": "patch-package",
        "build": "tsx scripts/build.ts",
        "_format": "prettier '**/*.{ts,tsx,json,md}'",
        "format": "yarn _format --write",
        "format:check": "yarn _format --list-different",
        "size": "bash ./scripts/compute-payload.sh",
        "start-angular-example": "bash ./scripts/start-example.sh angular start",
        "start-angular-kitchensink-example": "bash ./scripts/start-example.sh angular-kitchensink start",
        "start-react-router-framework-example": "bash ./scripts/start-example.sh react-router-framework dev",
        "start-react-router-data-example": "bash ./scripts/start-example.sh react-router-data dev",
        "start-react-router-declarative-example": "bash ./scripts/start-example.sh react-router-declarative dev",
        "start-tanstack-start-example": "bash ./scripts/start-example.sh tanstack-start dev",
        "start-tanstack-router-file-router-example": "bash ./scripts/start-example.sh tanstack-router-file-router dev"
    },
    "lint-staged": {
        "*.{ts,tsx,json,md}": [
            "prettier --write"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged -v"
        }
    },
    "author": "u/garronej & u/ddecrulle",
    "license": "MIT",
    "keywords": [
        "OpenID Connect",
        "PKCE",
        "Authorization Code Flow",
        "OIDC",
        "keycloak",
        "keycloak-js",
        "react",
        "angular",
        "TanStack Start"
    ],
    "homepage": "https://docs.oidc-spa.dev/",
    "dependencies": {},
    "peerDependencies": {
        "@types/react": "*",
        "react": "*",
        "rxjs": "*",
        "@angular/core": "*",
        "@angular/router": "*",
        "@angular/common": "*",
        "@tanstack/react-start": "*",
        "@tanstack/react-router": "*"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "@types/react": {
            "optional": true
        },
        "rxjs": {
            "optional": true
        },
        "@angular/core": {
            "optional": true
        },
        "@angular/router": {
            "optional": true
        },
        "@angular/common": {
            "optional": true
        },
        "@tanstack/react-start": {
            "optional": true
        },
        "@tanstack/react-router": {
            "optional": true
        }
    },
    "devDependencies": {
        "@babel/core": "7.24.9",
        "@babel/preset-env": "7.24.8",
        "@types/node": "^20.2.1",
        "@types/react": "19.1.10",
        "babel-loader": "9.1.3",
        "evt": "2.5.9",
        "husky": "^4.3.8",
        "lint-staged": "^11.1.1",
        "oidc-client-ts": "3.1.0",
        "patch-package": "^8.0.0",
        "prettier": "^2.8.8",
        "punycode": "^2.3.1",
        "jose": "5.9.4",
        "react": "19.1.1",
        "ts-node": "^10.9.1",
        "tsafe": "1.8.12",
        "tsx": "^4.15.5",
        "typescript": "5.7.2",
        "webpack": "5.93.0",
        "webpack-cli": "5.1.4",
        "zod": "3.24.2",
        "rxjs": "^7.8.2",
        "@angular/core": "^20.3.1",
        "@angular/router": "^20.3.1",
        "@angular/common": "^20.3.1",
        "@angular/platform-browser": "^20.3.1",
        "@tanstack/react-start": "^1.132.0",
        "esbuild": "^0.25.10",
        "magic-string": "^0.30.19",
        "vite": "^7.1.10",
        "@babel/parser": "^7.28.4",
        "@babel/generator": "^7.28.3",
        "@babel/types": "^7.28.4",
        "@types/babel__generator": "^7.6.4",
        "@babel/traverse": "^7.28.4",
        "@types/babel__traverse": "^7.28.0",
        "js2mjs": "^1.0.1",
        "ts-add-js-extension": "^1.6.6",
        "webcrypto-liner-shim": "1.4.8"
    },
    "publishConfig": {
        "access": "public"
    },
    "sideEffects": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/esm/index.mjs",
    "exports": {
        ".": {
            "module": "./dist/esm/index.mjs",
            "import": "./dist/esm/index.mjs",
            "default": "./dist/index.js"
        },
