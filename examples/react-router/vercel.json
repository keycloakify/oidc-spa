{
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "Cache-Control",
                    "value": "max-age=0, must-revalidate"
                }
            ]
        }
    ],
    "trailingSlash": false,
    "cleanUrls": true
}
